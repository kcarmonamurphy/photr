<span class="tw-flex tw-items-center">
  <span class="tw-text-gray-500 tw-mx-2">
    /
  </span>
  {{#each breadcrumbs as |breadcrumb index|}}
    {{#if index}}
      <span class="tw-text-gray-500 tw-mx-2">
        /
      </span>
    {{/if}}
    <LinkTo @route="authenticated.path.wildcard" @model={{breadcrumb.url}} class="tw-text-gray-500">
      {{breadcrumb.name}}
    </LinkTo>
  {{/each}}
</span>
